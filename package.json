{
  "name": "@samwen/oas-tools",
  "version": "2.1.8",
  "description": "From isa-group/oas-tools: NodeJS module to manage RESTful APIs defined with OpenAPI 3.0 Specs over express servers",
  "main": "src/index.js",
  "scripts": {
    "test": "snyk test && mocha ./tests/basic.js",
    "test:tdd": "mocha ./tests/basic.js -w",
    "lint": "eslint src/**/*.js Gruntfile.js tests/**/*.js",
    "coverage": "nyc mocha ./tests/basic.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "publish-please": "publish-please",
    "prepublishOnly": "publish-please guard"
  },
  "author": "ISA Group, University of Seville",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samswen/oas-tools.git"
  },
  "snyk": true,
  "nyc": {
    "exclude": [
      "node_modules",
      "tests/**/*.js"
    ],
    "reporter": [
      "json",
      "html",
      "text",
      "lcovonly"
    ]
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "accesscontrol-middleware": "^1.0.3",
    "body-parser": "^1.19.0",
    "fs": "0.0.1-security",
    "js-yaml": "^3.13.1",
    "json-schema-deref": "^0.5.0",
    "json-schema-deref-sync": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "lodash-compat": "^3.10.2",
    "multer": "^1.4.2",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "snyk": "^1.298.1",
    "validator": "^12.2.0",
    "whatwg-mimetype": "^2.3.0",
    "winston": "^3.2.1",
    "z-schema": "^4.2.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "http": "0.0.0",
    "jsdoc": "^3.6.3",
    "mocha": "^7.1.0",
    "mongoose": "^5.9.4",
    "nyc": "^15.0.0",
    "publish-please": "^5.5.1"
  }
}
